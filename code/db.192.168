;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	localhost. root.localhost. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.cuc.edu.cn.
128.21	IN	PTR	ns.cuc.edu.cn.
128.15	IN	PTR	wp.sec.cuc.edu.cn

